<AnchorPane prefHeight="760.0" prefWidth="1360.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.LoginController">
   <children>
      <Rectangle fill="#9ea7aa" height="54.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="1360.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <Rectangle fill="#cfd8dc" height="34.0" layoutY="54.0" stroke="TRANSPARENT" strokeType="INSIDE" strokeWidth="0.0" width="1360.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <JFXHamburger layoutX="25.0" layoutY="17.0" />
      <JFXButton fx:id="searchbutton" buttonType="RAISED" onAction="#search" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: WHITE;" text="search" AnchorPane.rightAnchor="0.0">
         <ripplerFill>
            <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </RadialGradient>
         </ripplerFill>
         <font>
            <Font name="Apple SD Gothic Neo Thin" size="13.0" />
         </font>
      </JFXButton>
      <HBox alignment="TOP_CENTER" layoutX="9.0" prefHeight="88.0" prefWidth="1360.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Login" textFill="WHITE">
               <font>
                  <Font name="Apple SD Gothic Neo Thin" size="24.0" />
               </font>
               <HBox.margin>
                  <Insets top="10.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="88.0">
         <children>
            <JFXTextField fx:id="LoginUserUsernameTextField" alignment="CENTER" maxWidth="350.0" prefWidth="350.0" promptText="username">
               <font>
                  <Font name="Apple SD Gothic Neo Thin" size="13.0" />
               </font>
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXPasswordField fx:id="LoginUserPasswordTextField" alignment="CENTER" maxWidth="350.0" prefWidth="350.0" promptText="password">
               <font>
                  <Font name="Apple SD Gothic Neo Thin" size="13.0" />
               </font>
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </JFXPasswordField>
            <Label text="I'm a: ">
               <font>
                  <Font name="Apple SD Gothic Neo Thin" size="13.0" />
               </font>
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </Label>
            <JFXRadioButton fx:id="ManRadioButton" text="manufacturer">
               <font>
                  <Font name="Apple SD Gothic Neo Thin" size="13.0" />
               </font>
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </JFXRadioButton>
            <JFXRadioButton fx:id="AgentRadioButton" text="agent">
               <font>
                  <Font name="Apple SD Gothic Neo Thin" size="13.0" />
