               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </JFXRadioButton>
            <JFXButton fx:id="LoginUserLoginButton" buttonType="RAISED" onAction="#login" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: WHITE;" text="log in">
               <ripplerFill>
                  <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
                     <stops>
                        <Stop color="BLACK" />
                        <Stop color="WHITE" offset="1.0" />
                     </stops>
                  </RadialGradient>
               </ripplerFill>
               <font>
                  <Font name="Apple SD Gothic Neo Thin" size="13.0" />
               </font>
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </JFXButton>
         </children>
      </VBox>
   </children>
</AnchorPane>
