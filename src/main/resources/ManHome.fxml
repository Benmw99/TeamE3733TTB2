<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.paint.RadialGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="760.0" prefWidth="1360.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.ManHomeController">
   <children>
      <Pane prefHeight="759.0" prefWidth="1360.0" style="-fx-background-color: WHITE;">
         <children>
            <Pane layoutX="-6.0" layoutY="-1.0" prefHeight="89.0" prefWidth="1367.0" style="-fx-background-color: #9ea7aa;">
               <children>
                  <Text fill="WHITE" layoutX="547.0" layoutY="53.0" strokeWidth="0.0" text="My Applications">
                     <font>
                        <Font name="Apple SD Gothic Neo Thin" size="36.0" />
                     </font>
                  </Text>
               </children>
            </Pane>
            <Pane layoutX="-1.0" layoutY="88.0" prefHeight="673.0" prefWidth="559.0" style="-fx-background-color: #cfd8dc;">
               <children>
                  <JFXTextField fx:id="searchAppsTextField" layoutX="14.0" layoutY="14.0" prefHeight="27.0" prefWidth="377.0" style="-fx-background-color: WHITE;" text="Search Applications">
                     <font>
                        <Font name="Apple SD Gothic Neo Thin" size="18.0" />
                     </font></JFXTextField>
                  <JFXButton fx:id="newFormButton" layoutX="411.0" layoutY="18.0" onAction="#addForm" style="-fx-background-radius: 15; -fx-background-color: #800020;" text="+" textFill="WHITE" />
                  <JFXButton fx:id="refreshButton" buttonType="RAISED" layoutX="463.0" layoutY="14.0" onAction="#refreshForms" style="-fx-background-color: #800020;" text="Refresh" textFill="WHITE">
                     <ripplerFill>
                        <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
                           <stops>
                              <Stop color="BLACK" />
                              <Stop color="WHITE" offset="1.0" />
                           </stops>
                        </RadialGradient>
                     </ripplerFill>
                     <font>
                        <Font name="Apple SD Gothic Neo Thin" size="18.0" />
                     </font>
                  </JFXButton>
               </children>
            </Pane>
            <Pane layoutX="558.0" layoutY="88.0" prefHeight="64.0" prefWidth="803.0" style="-fx-background-color: #cfd8dc;">
               <children>
                  <JFXButton fx:id="commentsButton" buttonType="RAISED" layoutX="326.0" layoutY="15.0" onAction="#viewAgentComments" style="-fx-background-color: #800020;" text="View Agent Comments" textFill="WHITE">
                     <ripplerFill>
                        <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
                           <stops>
                              <Stop color="BLACK" />
                              <Stop color="WHITE" offset="1.0" />
                           </stops>
                        </RadialGradient>
                     </ripplerFill>
                     <font>
                        <Font name="Apple SD Gothic Neo Thin" size="18.0" />
                     </font>
                  </JFXButton>
                  <JFXToggleButton fx:id="editToggle" layoutX="133.0" layoutY="2.0" text="Edit">
                     <font>
                        <Font name="Apple SD Gothic Neo Thin" size="18.0" />
                     </font>
                  </JFXToggleButton>
                  <JFXButton fx:id="downloadButton" buttonType="RAISED" layoutX="551.0" layoutY="15.0" onAction="#download" style="-fx-background-color: #800020;" text="Download" textFill="WHITE">
                     <font>
                        <Font name="Apple SD Gothic Neo Thin" size="18.0" />
                     </font>
                     <ripplerFill>
                        <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
                           <stops>
                              <Stop color="BLACK" />
                              <Stop color="WHITE" offset="1.0" />
                           </stops>
                        </RadialGradient>
                     </ripplerFill>
                  </JFXButton>
                  <JFXButton fx:id="printButton" buttonType="RAISED" layoutX="680.0" layoutY="15.0" onAction="#print" style="-fx-background-color: #800020;" text="Print" textFill="WHITE">
                     <ripplerFill>
                        <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
                           <stops>
                              <Stop color="BLACK" />
                              <Stop color="WHITE" offset="1.0" />
                           </stops>
                        </RadialGradient>
                     </ripplerFill>
                     <font>
                        <Font name="Apple SD Gothic Neo Thin" size="18.0" />
                     </font>
                  </JFXButton>
               </children>
            </Pane>
         </children></Pane>
      <HBox layoutX="15.0" layoutY="200.0" prefHeight="575.0" prefWidth="475.0">
         <children>
            <fx:include source="TableView.fxml" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="-150.0" AnchorPane.topAnchor="150.0">
         <children>
            <fx:include source="FormDisplay.fxml" />
         </children>
      </HBox>
      <fx:include source="MenuDrawer.fxml" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
   </children>
</AnchorPane>
